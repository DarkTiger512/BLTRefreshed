        <!-- Send Message In Game -->
        <string id="FFvfbTgo" text="Отправить сообщение в игре"/>
        <!-- Retire My Hero -->
        <string id="AZMDg7St" text="На пенсию моего героя"/>
        <!-- 50k XP -->
        <string id="uA1yEnyO" text="50 тыс. Опыта"/>
        <!-- Burn Player -->
        <string id="2ciF79zR" text="Записать игрока"/>
        <!-- Heal Hero -->
        <string id="likTlNpu" text="Исцелить героя"/>
