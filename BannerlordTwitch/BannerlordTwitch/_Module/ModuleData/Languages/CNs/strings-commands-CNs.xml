        <!-- show gold -->
        <string id="zdRj4blL" text="秀金"/>
        <!-- gold -->
        <string id="W5uX9HCJ" text="金子"/>

        <!-- retinuelist -->
        <!-- Shows your current retinue troops. -->
        <string id="BlBiEmbf" text="显示您当前的随从部队。"/>
        <!-- retinue list -->
        <string id="ysbFV5uT" text="随从名单"/>
        <!-- retinuelist -->
        <string id="kG6yjBBR" text="随从"/>

        <!-- adopt -->
        <!-- Get a hero in game. Almost all other actions require an adopted hero. -->
        <string id="MpvqvKef" text="在游戏中获得英雄。几乎所有其他行动都需要一个被收养的英雄。"/>
        <!-- adopt -->
        <string id="0grO6hB5" text="采纳"/>

        <!-- nameitem -->
        <!-- You can use this to name your custom items.&lt;br&gt;Usage is &lt;code&gt;!nameitem (name)&lt;/code&gt;, for example &lt;code&gt;!nameitem Foehammer&lt;/code&gt;.&lt;br&gt;If you call &lt;code&gt;!nameitem&lt;/code&gt; on its own it will tell you what item will be named next. -->
        <string id="S1FZXQim" text="您可以使用它来命名您的自定义项目。&lt;br&gt;用法是&lt;code&gt;!nameitem (name)&lt;/code&gt; ，例如&lt;code&gt;!nameitem Foehammer&lt;/code&gt; 。&lt;br&gt;如果您&lt;code&gt;!nameitem&lt;/code&gt; ，它会告诉您接下来将命名哪个项目。"/>
        <!-- name a reward item -->
        <string id="tNVmXpoc" text="命名奖励项目"/>
        <!-- nameitem -->
        <string id="IFDecKha" text="名称项"/>

        <!-- bltbet -->
        <!-- Use this to bet on tournament matches, when available.&lt;br&gt;Usage is &lt;code&gt;!bltbet (team) (gold)&lt;/code&gt;, for example &lt;code&gt;!bltbet red 10000&lt;/code&gt; to bet 10000 gold on red. Winnings are divided up from the whole pot, based on relative bet size.&lt;br&gt;If only one team is bet upon, the bets will be refunded. -->
        <string id="thajdk5s" text="如果可用，使用它来投注锦标赛比赛。&lt;br&gt;用法是&lt;code&gt;!bltbet (team) (gold)&lt;/code&gt; ，例如&lt;code&gt;!bltbet red 10000&lt;/code&gt;在红色上下注 10000 金币。根据相对下注大小，从整个底池中分配奖金。&lt;br&gt;如果只投注一支球队，投注将被退还。"/>
        <!-- bet on tournament -->
        <string id="xBgLXaf7" text="投注锦标赛"/>
        <!-- bltbet -->
        <string id="haKiwbsR" text="打赌"/>

